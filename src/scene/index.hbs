<a-scene id="aframe-project" 
  auto-enter-vr 
  inspector="url: https://aframe.io/releases/0.3.0/aframe-inspector.min.js">
  <!-- assets + templates -->
  <a-assets id="assets">
    <img src="https://img.gs/bbdkhfbzkk/2048x1024,stretch/http://i.imgur.com/WMNH2OF.jpg" id="chrome" crossorigin="anonymous">
    <audio id="song" src="assets/audio/swang.mp3" preload="auto"></audio>
  </a-assets>

  <!-- camera + cursor -->
  {{> camera}}

  <!-- sky -->
  <a-gradient-sky material="shader: gradient; topColor: 235 210 243; bottomColor: 180 251 255;"></a-gradient-sky>

